<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Rewrite Seven Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}-{R:3}-{R:4}-{R:5}-{R:6}-{R:7}-{R:8}" />
        </rule>
        <rule name="Rewrite Six Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}-{R:3}-{R:4}-{R:5}-{R:6}-{R:7}" />
        </rule>
        <rule name="Rewrite Five Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}-{R:3}-{R:4}-{R:5}-{R:6}" />
        </rule>
        <rule name="Rewrite Quad Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}-{R:3}-{R:4}-{R:5}" />
        </rule>
        <rule name="Rewrite Triple Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}-{R:3}-{R:4}" />
        </rule>
        <rule name="Rewrite Double Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}-{R:3}" />
        </rule>
        <rule name="Rewrite Single Underscores to Dashes" stopProcessing="true">
          <match url="([A-Za-z0-9\-_/]+)_([A-Za-z0-9\-_/]+)$" />
          <action type="Redirect" url="{R:1}-{R:2}" />
        </rule>
