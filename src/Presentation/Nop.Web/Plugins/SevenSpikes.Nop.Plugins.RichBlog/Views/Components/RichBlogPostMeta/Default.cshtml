@model RichBlogMetaModel

@if(!string.IsNullOrEmpty(Model.BlogPostImageUrl))
{
    <meta property="og:image" content="@Model.BlogPostImageUrl"/>
}

@if (!string.IsNullOrEmpty(Model.BlogPostUrl))
{
    <meta property="og:url" content="@Model.BlogPostUrl.Replace("%2F", "/")" />
}

@if (!string.IsNullOrEmpty(Model.BlogPostTitle))
{
    <meta property="og:title" content="@Model.BlogPostTitle" />
}